		<plexus.version>3.0.22</plexus.version>
		<pmd.version>3.4</pmd.version>
		<properties.version>1.0-alpha-2</properties.version>
		<quartz.version>2.2.1</quartz.version>
		<site.version>3.4</site.version>
		<slf4j.version>1.7.12</slf4j.version>
		<stripe.version>1.28.0</stripe.version>
		<surfire.version>2.18.1</surfire.version>
		<tag.version>2.4</tag.version>
		<versions.version>2.2</versions.version>
